<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-set-get-status-path">
    <DisplayName>AM-set-get-status-path</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>target.copy.pathsuffix</Name>
        <Value>false</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>statusPath</Name>
        <Template>/v2/dialTo={request.header.x-dial-to}/every={request.header.x-every}/target={target.name}</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>