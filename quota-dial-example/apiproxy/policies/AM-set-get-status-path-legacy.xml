<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-set-get-status-path-legacy">
    <DisplayName>AM-set-get-status-path-legacy</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>target.copy.pathsuffix</Name>
        <Value>false</Value>
        <Ref/>
    </AssignVariable>
    <AssignVariable>
        <Name>statusPathLegacy</Name>
        <Template>/v1/dialTo={request.header.x-dial-to}/every={request.header.x-every}/target={target.name}/allowed={ratelimit.QU-dial.allowed.count}/used={ratelimit.QU-dial.used.count}/avail={ratelimit.QU-dial.available.count}</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>